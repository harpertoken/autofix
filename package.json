  "scripts": {
    "dev": "NODE_ENV=development tsx apps/server/index.ts",
    "build": "vite build --mode production && esbuild apps/server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=api/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "npx tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:report": "npx playwright show-report",
    "analyze": "npx vite-bundle-analyzer dist/stats.html",
    "format": "prettier --write .",
    "duplicate-check": "jscpd",
    "preflight": "npm run prepare && npm run format && npm run check && npm run test:coverage && npm run duplicate-check && npm run build",
    "prepare": "husky"
  },
