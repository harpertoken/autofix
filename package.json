{
  "name": "autofix",
  "version": "1.0.0",
  "description": "AI-powered text autofix with CLI and web client.",
  "author": "harpertoken",
  "repository": "https://github.com/harpertoken/autofix",
  "type": "module",
  "license": "MIT",
